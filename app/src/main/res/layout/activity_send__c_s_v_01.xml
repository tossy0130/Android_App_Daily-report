<?xml version="1.0" encoding="utf-8"?>
    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:isScrollContainer="false"
        android:id="@+id/Send_CSV_01"
        tools:context=".Send_CSV_01">

    <LinearLayout
        android:id="@+id/send_const_01"
        android:orientation="vertical"
        android:layout_height="fill_parent"
        android:layout_width="fill_parent"
        android:paddingBottom="30dp"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/send_const_02"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/result_view"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:textSize="18dp"
                android:textColor="@color/menu_color"
                android:layout_marginHorizontal="30dp"
                android:layout_marginVertical="5dp"
                />

            <TextView
                android:id="@+id/test_csv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="15dp"
                android:layout_marginVertical="5dp"
                app:layout_constraintTop_toBottomOf="@+id/result_view"
                app:layout_constraintLeft_toLeftOf="parent"
                />



    <Button
        android:id="@+id/send_csv_btn_001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/test_csv"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="ファイル送信"
        android:layout_marginHorizontal="30dp"

        />

</androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

</ScrollView>